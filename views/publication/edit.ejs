<%- include("../partials/header.ejs")%>

<div class="row justify-content-center">
	<div class="col-lg-5 col-md-7 col-sm-10">



<form  class="p-5"  action="/faculties/<%=faculty._id%>/publications/<%=publication._id%>?_method=PUT" method="POST">
	<div class="form-group">
	<input type="text" class="form-control" placeholder="<%=publication.title%>" name="publication[title]">
	</div>
	<div class="form-group">
	<input type="number" class="form-control" placeholder="<%=publication.id%>" name="publication[id]">
	</div>
	<div class="form-group">
	<input type="text" class="form-control" placeholder="<%=publication.year%>" name="publication[year]">
	</div>
	<div class="form-group">
	<input type="text" class="form-control" placeholder="<%=publication.type%>" name="publication[type]">
	</div>
	<div class="form-group">
	<input type="text" class="form-control" placeholder="<%=publication.content%>" name="publication[content]">
	</div>
	<button class="btn btn-block btn-success">
		Submit
	</button>
</form>
		
